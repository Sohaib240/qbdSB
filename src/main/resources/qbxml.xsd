            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>

    <xsd:element name="SalesAndPurchaseMod">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="SalesDesc" minOccurs="0">
                    <xsd:simpleType>
                        <xsd:restriction base="STRTYPE">
