            <xsd:restriction base="ENUMTYPE">
                <xsd:enumeration value="ThisWeekToDate"/>
                <xsd:enumeration value="ThisMonthToDate"/>
                <xsd:enumeration value="ThisFiscalQuarterToDate"/>
                <xsd:enumeration value="ThisFiscalYearToDate"/>
