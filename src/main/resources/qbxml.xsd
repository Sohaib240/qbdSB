                    <xsd:restriction base="STRTYPE">
                        <xsd:maxLength value="50"/>
                    </xsd:restriction>
                </xsd:simpleType>
            </xsd:element>
        </xsd:sequence>
        <xsd:attribute name="requestID" type="STRTYPE"/>
    </xsd:complexType>



    <xsd:complexType name="CustomerQueryRsType">
        <xsd:sequence>
