    <xsd:element name="ChargeNumber" type="STRTYPE"/>
    <xsd:element name="OwnerID" type="GUIDTYPE"/>
    <xsd:element name="WageType">
        <xsd:simpleType>
            <xsd:restriction base="ENUMTYPE">
                <xsd:enumeration value="Hourly"/>
                <xsd:enumeration value="Salary"/>
            </xsd:restriction>
        </xsd:simpleType>
    </xsd:element>


